<table class="a">
    {{#each members}}
        <tbody>
        <tr>
        <th> Navn </th> <th> Alder </th> <th> Email </th>
        </tr>
        <tr>
            <td><h5>{{this.name}}</h5></td>
        </tr>
        <tr>
            <td>
                <p>{{this.age}}</p>
            </td>
        </tr>
        <tr>
            <td>
                <p>{{this.email}}</p>
            </td>

        <td>
            <button class="button1" id="{{_id}}" onclick="deleteMember('{{this._id}}')"> Slet</button>
        </td>
            <td>
                <button class="button2" id="{{_id}}"
                        onclick="editMember('{{this._id}}', '{{this.name}}', '{{this.age}}','{{this.email}}', '{{this.password}}')">
                    Ret
                </button>
            </td>

        </tr>

        </tbody>

    {{/each}}
</table>

